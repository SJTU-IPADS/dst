BODY { background-color: #FFFFFF; margin: 16; }

OL,UL,P,BODY,TD,TR,TH,FORM { font-family: verdana,arial,helvetica,sans-serif; font-size:12px; color: #333333; }

H1 { font-size: 20px; font-family: verdana,arial,helvetica,sans-serif; }
H2 { font-size: 18px; font-family: verdana,arial,helvetica,sans-serif; }
H3 { font-size: 16px; font-family: verdana,arial,helvetica,sans-serif; }
H4 { font-size: 14px; font-family: verdana,arial,helvetica,sans-serif; }
H5 { font-size: 12px; font-family: verdana,arial,helvetica,sans-serif; }
H6 { font-size: 10px; font-family: verdana,arial,helvetica,sans-serif; }

PRE,TT { font-family: courier,sans-serif }

A { text-decoration: none; color: #6666FF; }
A:visited { text-decoration: none; color: #6666AA; }
A:link { text-decoration: none; color: #6666AA; }
A:active { text-decoration: none; color: #6666AA; }
A:hover { text-decoration: underline; color: #FF6666 }

SPAN.center { text-align: center }
SPAN.boxspace { font-size: 2pt; }

A.maintitlebar { color: #FFFFFF }
A.maintitlebar:visited { color: #FFFFFF }

A.sortbutton { color: #FFFFFF; text-decoration: underline; }
A.sortbutton:visited { color: #FFFFFF; text-decoration: underline; }

A.menus { color: #000000; text-decoration: none; }
A.menus:visited { color: #000000; text-decoration: none; }

A.tabs { color: #000000; text-decoration: underline; }
A.tabs:visited { color: #000000; text-decoration: underline; }

SPAN.alignright { text-align: right }
SPAN.maintitlebar { font-size: 12px; color: #FFFFFF }
SPAN.titlebar { text-align: center; font-size: 12px; color: #FFFFFF; font-weight: bold }
TD.featurebox { font-size: 12px; }
